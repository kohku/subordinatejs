{
  "name": "subordinatejs",
  "version": "1.0.0-alpha.1",
  "description": "Module to help encapsulate method invocation",
  "type": "commonjs",
  "main": "bin/index.js",
  "types": "bin/types/index.d.js",
  "bin": {
    "subordinate": "bin/index.js"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:bin",
    "build:clean": "rimraf bin",
    "build:bin": "rollup -c --bundleConfigAsCjs",
    "build:watch": "npm run build -- --watch",
    "start": "ts-node src/index.ts",
    "lint": "eslint ./src/**/*.ts",
    "test": "jest --runInBand --verbose",
    "prepublish": "npm run build",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kohku/subordinatejs.git"
  },
  "files": [
    "bin"
  ],
  "keywords": [
    "Command",
    "Invoker",
    "Encapsulate",
    "Method",
    "Invocation"
  ],
  "author": "Daniel Cruz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kohku/subordinatejs/issues"
  },
  "homepage": "https://github.com/kohku/subordinatejs#readme",
  "lint-staged": {
    "*.ts": "prettier --write --ignore-unknown",
    "**/src/**/*.ts": "eslint"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0"
  }
}
